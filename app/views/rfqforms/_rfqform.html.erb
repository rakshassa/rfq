<div id="parent">
	<li id="rfqform_<%= rfqform.id %>">
	  
	  <%= link_to rfqform.id.to_s.rjust(4, '0'), edit_rfqform_path(rfqform), id: "keep_link" %>
	  &nbsp&nbsp&nbsp&nbsp

	  <%=  rfqform.date.to_s  %>
	  &nbsp&nbsp&nbsp&nbsp

	  <%=  Part.find(rfqform.program).name.to_s  %>
	  &nbsp&nbsp&nbsp&nbsp

	  <small>
	    <%= link_to "Delete", rfqform, method: :delete,
	                data: { confirm: "Are you sure?" }, remote: true %>
	    &nbsp&nbsp&nbsp&nbsp
        <%= link_to "Print", rfqform_path(rfqform.id, :format => :pdf), id: "keep_link" %>	                
        &nbsp&nbsp&nbsp&nbsp
        
        <% if !rfqform.built %>
        <%= link_to "Build", build_rfqform_path(rfqform.id), method: :post %>
        <% end %>
        
	  </small>
	  
	</li>
</div>
<ul id="children">
	<% if @quotes.key?(rfqform.id) %>
		<table class="table table-striped table-bordered">
	        <tr>
	          <th>RFQ ID</th>
	          <th class="vendor">Vendor</th>
	          <th class="program">Program</th>
	          <th class="part">Part</th>
	          <th class="date_sent">Date Sent</th>
	          <th class="quote_num">Quote #</th>
	          <th class="status">Status</th>
	        </tr>			
			<% @quotes[rfqform.id].each do |q| %>
				<%= render q, locals: { q => q } %>
			<% end %>

		</table>
	<% end %>
	
	
</ul>